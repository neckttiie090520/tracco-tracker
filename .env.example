# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Admin Service Role Key (for server-side operations)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Session Management Configuration (Optional)
# Enable/disable session management (default: true)
# REACT_APP_SESSION_MANAGEMENT_ENABLED=true

# Maximum idle time before auto-logout in milliseconds (default: 1 hour)
# REACT_APP_SESSION_IDLE_TIME=3600000

# Total session timeout in milliseconds (default: 8 hours)
# REACT_APP_SESSION_TIMEOUT=28800000

# Time before timeout to show warning in milliseconds (default: 5 minutes)
# REACT_APP_SESSION_WARNING_TIME=300000
