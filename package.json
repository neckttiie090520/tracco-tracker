{
  "name": "workshop-tracker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && open dist/stats.html",
    "build:production": "node scripts/clean-logs.js && vite build",
    "clean:logs": "node scripts/clean-logs.js",
    "analyze:unused": "node scripts/find-unused.js",
    "analyze:deps": "npx depcheck",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test tests/e2e/performance.spec.ts",
    "test:queries": "playwright test tests/e2e/database-queries.spec.ts",
    "test:live": "playwright test tests/e2e/live-performance.spec.ts --headed",
    "test:data": "playwright test tests/e2e/performance-with-data.spec.ts --headed --timeout=120000"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.38.5",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.3",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "minidenticons": "^4.2.1",
    "p5": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.0",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.7",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
